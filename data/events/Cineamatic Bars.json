{
    "params": [
        {
            "name": "Tween Bars?",
            "type": "Bool",
            "defaultValue": true
        },
        {
            "name": "New Size (0 for not visible, 1 for fully take up screen)",
            "type": "Float(0, 1, 0.01, 2)",
            "defaultValue": "0.2"
        },
        {
            "name": "Tween Time (Steps)",
            "type": "Float(0.25, 9999, 0.25, 2)",
            "defaultValue": "4"
        },
		{
            "name": "Tween Ease (ex: circ, quad, cube)",
            "type": "DropDown('linear', 'back', 'bounce', 'circ', 'cube', 'elastic', 'expo', 'quad', 'quart', 'quint', 'sine', 'smoothStep', 'smootherStep')",
            "defaultValue": "linear"
        },
        {
            "name": "Tween Type (ex: InOut)",
            "type": "DropDown('In', 'Out', 'InOut')",
            "defaultValue": "In"
        }
    ]
}